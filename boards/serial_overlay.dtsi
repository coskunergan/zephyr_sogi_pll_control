
&usart3 {
	status = "okay";

	w1_0: w1-zephyr-serial-0 {
		compatible = "zephyr,w1-serial";
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		ds18b20 {
			compatible = "maxim,ds18b20";
			family-code = <0x28>;
			resolution = <12>;
			status = "okay";
		};
	};
};

&adc1 {
	pinctrl-0 = <&adc_in10_pc0 &adc_in6_pa6 &adc_in7_pa7>;
	pinctrl-names = "default";
	status = "okay";
};


