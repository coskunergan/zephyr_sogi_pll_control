
&usart3 {
	status = "okay";

	w1_0: w1-zephyr-serial-0 {
		compatible = "zephyr,w1-serial";
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		ds18b20 {
			compatible = "maxim,ds18b20";
			family-code = <0x28>;
			resolution = <12>;
			status = "okay";
		};
	};
};

&adc1 {
	pinctrl-0 = <&adc1_in10_pc0>;
	pinctrl-names = "default";
	st,adc-prescaler = <2>;
	status = "okay";
};

&dac1 {
	pinctrl-0 = <&dac_out2_pa5>;
	pinctrl-names = "default";	
	status = "okay";
};

&i2c2{
	pinctrl-0 = <&i2c2_scl_pb10 &i2c2_sda_pb11>;
	pinctrl-names = "default";
	status = "okay";	
};

